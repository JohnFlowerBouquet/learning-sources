{
  "name": "learning-sources",
  "version": "1.0.0",
  "description": "Free Learning Sources List",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production"
  },
  "author": "John Flower Bouquet",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.5",
    "eslint": "^4.19.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    },
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JohnFlowerBouquet/learning-sources.git"
  },
  "bugs": {
    "url": "https://github.com/JohnFlowerBouquet/learning-sources/issues"
  },
  "homepage": "https://github.com/JohnFlowerBouquet/learning-sources#readme",
  "dependencies": {
    "eslint-plugin-immutable": "^1.0.0"
  }
}
