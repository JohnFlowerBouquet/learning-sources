{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true,
    "commonjs": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "plugins": ["immutable"],
  "rules": {
    "no-console": 0
  }
}
